# ahrs_object
class Ahrs:
    def __init__(self) -> None: ...
    @property
    def settings(self) -> None: ...
    @settings.setter
    def settings(self, value: AhrsSettings) -> None: ...
    @property
    def sample_period(self) -> None: ...
    @sample_period.setter
    def sample_period(self, value: float) -> None: ...
    @property
    def quaternion(self) -> np.ndarray: ...
    @quaternion.setter
    def quaternion(self, value: np.ndarray) -> None: ...
    @property
    def gravity(self) -> np.ndarray: ...
    @property
    def linear_acceleration(self) -> np.ndarray: ...
    @property
    def earth_acceleration(self) -> np.ndarray: ...
    @property
    def internal_states(self) -> AhrsInternalStates: ...
    @property
    def flags(self) -> AhrsFlags: ...
    @property
    def heading(self) -> None: ...
    @heading.setter
    def heading(self, value: float) -> None: ...
    def reset(self) -> None: ...
    def update(self, gyroscope: np.ndarray, accelerometer: np.ndarray, magnetometer: np.ndarray) -> None: ...
    def update_no_magnetometer(self, gyroscope: np.ndarray, accelerometer: np.ndarray) -> None: ...
    def update_external_heading(self, gyroscope: np.ndarray, accelerometer: np.ndarray, heading: float) -> None: ...

# ahrs_flags_object
class AhrsFlags:
    @property
    def initialising(self) -> bool: ...
    @property
    def angular_rate_recovery(self) -> bool: ...
    @property
    def acceleration_recovery(self) -> bool: ...
    @property
    def magnetic_recovery(self) -> bool: ...

# ahrs_internal_states_object
class AhrsInternalStates:
    @property
    def acceleration_error(self) -> float: ...
    @property
    def accelerometer_ignored(self) -> bool: ...
    @property
    def acceleration_recovery_trigger(self) -> float: ...
    @property
    def magnetic_error(self) -> float: ...
    @property
    def magnetometer_ignored(self) -> bool: ...
    @property
    def magnetic_recovery_trigger(self) -> float: ...

# ahrs_settings_object
class AhrsSettings:
    def __init__(
        self,
        convention: int = ...,
        gain: float = ...,
        gyroscope_range: float = ...,
        acceleration_rejection: float = ...,
        magnetic_rejection: float = ...,
        recovery_trigger_period: int = ...,
    ) -> None: ...
    @property
    def convention(self) -> int: ...
    @convention.setter
    def convention(self, value: int) -> None: ...
    @property
    def gain(self) -> float: ...
    @gain.setter
    def gain(self, value: float) -> None: ...
    @property
    def gyroscope_range(self) -> float: ...
    @gyroscope_range.setter
    def gyroscope_range(self, value: float) -> None: ...
    @property
    def acceleration_rejection(self) -> float: ...
    @acceleration_rejection.setter
    def acceleration_rejection(self, value: float) -> None: ...
    @property
    def magnetic_rejection(self) -> float: ...
    @magnetic_rejection.setter
    def magnetic_rejection(self, value: float) -> None: ...
    @property
    def recovery_trigger_period(self) -> int: ...
    @recovery_trigger_period.setter
    def recovery_trigger_period(self, value: int) -> None: ...

# bias_object
class Bias:
    def __init__(self) -> None: ...
    @property
    def settings(self) -> None: ...
    @settings.setter
    def settings(self, value: BiasSettings) -> None: ...
    def update(self, gyroscope: np.ndarray) -> np.ndarray: ...

# bias_settings_object
class BiasSettings:
    def __init__(
