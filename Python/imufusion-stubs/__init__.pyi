    @property
    def stationary_threshold(self) -> float: ...
    @stationary_threshold.setter
    def stationary_threshold(self, value: float) -> None: ...
    @property
    def stationary_period(self) -> float: ...
    @stationary_period.setter
    def stationary_period(self, value: float) -> None: ...
